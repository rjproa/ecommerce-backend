{
  "kind": "collectionType",
  "collectionName": "clientes",
  "info": {
    "singularName": "cliente",
    "pluralName": "clientes",
    "displayName": "Cliente"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "password": {
      "type": "string",
      "minLength": 8,
      "required": true
    },
    "codigo": {
      "type": "uid",
      "required": true,
      "minLength": 5,
      "maxLength": 5
    },
    "nombre": {
      "type": "string",
      "required": true
    },
    "habilitado": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "compras": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "ultimaCompra": {
      "type": "date",
      "required": false
    },
    "puntos": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "opcion": {
      "type": "integer",
      "default": 1
    }
  }
}
